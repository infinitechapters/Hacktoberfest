#include <iostream>
using namespace std;

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n=nums.size();
        int j=1;
        for(int i=1;i<n;i++){
            if(nums[i]!=nums[i-1]){
                nums[j]=nums[i];
                j++;
            }
        }
        return j;
    }
};

int main(){
Solution sol;
 vector<int> nums = {1, 1, 2, 2, 3, 4, 4, 5};
int ans=sol.removeDuplicates(nums);
cout<<"The required array is:"<<ans<<endl;
return 0;
}
